{
    "minimum-stability": "dev",
    "name": "phergie/phergie-slack-connection",
    "description": "Data structure for containing information about a Slack client connection, analogous to Phergie\Irc\Connection",
    "keywords": [ "slack", "client", "server", "connection" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/alexmace/phergie-slack-connection/issues",
        "source": "https://github.com/alexmace/phergie-slack-connection"
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Slack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Slack\\Tests\\": "tests"
        }
    }
}
